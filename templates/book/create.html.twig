{% extends 'base.html.twig' %}
{% block body %}
    {% if error is defined and error != false %}
        {{ error }}
    {% endif %}
    <div class="col-12">
        {{ form_start(form, {'attr': {'class': 'd-flex align-items-end gap-3 w-100'}}) }}
        {{ form_row(form.type) }}
        {{ form_end(form) }}
    </div>
{% endblock %}